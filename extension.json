{
	"name": "VEForPageForm",
	"version": "0.1.0",
	"author": [
		"Pierre Boutet"
	],
	"url": "https://github.com/wikifab",
	"descriptionmsg": "visualeditorforpageform-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.28.0"
	},
	"callback": "VisualEditorHooks::onRegistration",
	"config": {
		"VEForPageForm-areaClasses": "vefpf-area"
	},
	"MessagesDirs": {
		"VEForPageForm": [
			"i18n"
		]
	},
	"Hooks": {
		"FormEdit::showEditForm:initial": [
			"VeForPageForm\\Hooks::start"
		]
	},
	"ResourceModules": {
		"ext.visualEditorForPageForm.init": {
			"scripts": [
				"modules/veforpageform.js"
			],
			"dependencies": [
			]
		},
		"ext.visualEditorForPageForm.visualEditor": {
			"scripts": [
				"modules/lib/ve/ve.init.sa.Target.js",
				"modules/pf-ve/ui/ui.CommandRegistry.js",
				"modules/pf-ve/ui/ui.SwitchEditorAction.js",
				"modules/pf-ve/ui/ui.SwitchEditorTool.js",
				"modules/pf-ve/ext.pageforms.ve.target.js",
				"modules/pf-ve/ext.pageforms.ve.editor.js"
			],
			"styles": [
				"modules/veforpageform.css"
			],
			"dependencies": [
				"ext.visualEditor.core",
				"ext.visualEditor.core.desktop",
				"ext.visualEditor.data",
				"ext.visualEditor.icons",
				"ext.visualEditor.mediawiki",
				"ext.visualEditor.desktopTarget",
				"ext.visualEditor.mwimage",
				"ext.visualEditor.mwlink",
				"ext.visualEditor.mwtransclusion",
				"oojs-ui.styles.icons-editing-advanced"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "VEForPageForm"
	},
	"DefaultUserOptions": {
		"visualeditorforpageform-enable": 1
	},
	"AutoloadClasses": {
		"VeForPageForm\\Hooks": "includes/Hooks.php"
	},
	"manifest_version": 1
}
